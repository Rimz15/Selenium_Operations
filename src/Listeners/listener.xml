<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNgListenerSuite">
<listeners>
<listener class-name = "Listeners.Listeners"/>
</listeners>
  <test name="LoginTest">
    <classes>
      <class name="Listeners.loginTest">
      <methods>
      <exclude name = "loginByEmail" />
      <include name = "loginByFacebook" />
      </methods>
      </class>
      </classes>
 </test> 
</suite> 